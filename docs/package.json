{
  "name": "docs",
  "version": "0.21.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "rimraf dist && next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "generate-dates": "tsx scripts/generate-dates.ts",
    "prebuild": "npm run generate-dates",
    "prepublishOnly": "npm run build",
    "publish:local": "npm version 0.0.0-$(date +%Y%m%d%H%M) --no-git-tag-version && npm publish --tag latest",
    "publish:local:next": "npm version 0.0.0-$(date +%Y%m%d%H%M) --no-git-tag-version && npm publish --tag next",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@databuddy/sdk": "^2.3.29",
    "@vendure-io/docs-provider": "^0.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "motion": "^12.33.0",
    "next": "^14.2.11",
    "next-sitemap": "^4.2.3",
    "nextra": "^3.2.5",
    "nextra-theme-docs": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@types/node": "^22.10.10",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.24",
    "eslint-config-custom": "*",
    "lucide-react": "^0.468.0",
    "postcss": "^8.5.6",
    "rimraf": "^5.0.5",
    "tailwindcss": "3",
    "tsx": "^4.7.0",
    "typescript": "5.8.2"
  }
}
